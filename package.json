    "react-burger-menu": "^1.10.9",
    "react-dom": "^15.4.1",
    "react-fa": "^4.1.2",
    "react-gravatar": "^2.6.1",
    "react-helmet": "^3.2.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-imgix": "^5.1.0",
    "react-immutable-proptypes": "^2.1.0",
